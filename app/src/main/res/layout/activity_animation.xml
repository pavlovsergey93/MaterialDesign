<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		xmlns:app="http://schemas.android.com/apk/res-auto">
	<androidx.fragment.app.FragmentContainerView
			android:id="@+id/aa_fragment_container"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toTopOf="@id/aa_bottom_navigation_view"/>

	<com.google.android.material.bottomnavigation.BottomNavigationView
			android:id="@+id/aa_bottom_navigation_view"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toBottomOf="@id/aa_fragment_container"
			app:menu="@menu/menu_animation_activity_bottom_navigation_view" />

</androidx.constraintlayout.widget.ConstraintLayout>